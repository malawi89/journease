<%= simple_form_for(@trip) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>
  <div class="form-inputs">
    <%= f.input :start_date %>
  </div>
  <div class="form-inputs">
    <%= f.input :end_date %>
  </div>

  <div class="form-actions text-center">
    <%= f.button :submit, class:"btn btn-success" %>

  </div>
<% end %>
